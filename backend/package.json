{
  "name": "sequelize-demo-backend",
  "version": "1.0.0",
  "description": "Express.js + Sequelize backend for CSIR EOI Demo",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "seed": "node src/seeders/seed.js",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:coverage": "jest --forceExit --detectOpenHandles --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "sqlite3": "^5.1.7"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterSetup": ["./tests/setup.js"],
    "testMatch": ["**/tests/**/*.test.js"],
    "verbose": true
  },
  "keywords": ["express", "sequelize", "postgresql", "rest-api", "csir-eoi"],
  "author": "AINEXIM",
  "license": "MIT"
}
